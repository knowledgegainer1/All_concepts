pipeline {
    agent {
        node {
            label 'agent1' // Corrected syntax: 'label' instead of '='
        }
    }
    environment{
        demo = "this is demo"
    }
    stages {
        stage('Build') { 
            steps {
                echo "thiis is "
            }
        }
        stage('Test') { 
            steps {
                echo "thiis is "
            }
        }
        stage('Deploy') { 
            steps {
               echo "thiis is "
               ecfo "$env.demo"
            }
        }
    }
}